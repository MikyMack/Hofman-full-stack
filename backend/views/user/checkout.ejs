            <script>
                // In your checkout page JavaScript
                document.addEventListener('DOMContentLoaded', function () {
                    // Coupon toggle
                    const couponToggle = document.querySelector('.show-coupon');
                    const couponContent = document.querySelector('.coupon-content');
    
                    if (couponToggle && couponContent) {
                        couponToggle.addEventListener('click', function (e) {
                            e.preventDefault();
                            couponContent.style.display = couponContent.style.display === 'none' ? 'block' : 'none';
                        });
                    }
    
                    // Apply coupon
                    const applyCouponBtn = document.querySelector('.btn-coupon');
                    if (applyCouponBtn) {
                        applyCouponBtn.addEventListener('click', async function () {
                            const couponCode = document.getElementById('coupon_code').value.trim();
    
                            if (!couponCode) {
                                alert('Please enter a coupon code');
                                return;
                            }
    
                            const response = await fetch('/apply-coupon', {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({ couponCode })
                            });
    
                            const result = await response.json();
    
                            if (result.success) {
                                // Update the order summary
                                location.reload(); // Simple refresh to update all values
                            } else {
                                alert(result.message || 'Failed to apply coupon');
                            }
                        });
                    }
                });
            </script>
